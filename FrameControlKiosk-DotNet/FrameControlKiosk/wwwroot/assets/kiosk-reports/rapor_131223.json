  "Controls": [
    {
      "Id": 16,
      "Title": "SEALİNG YÜZEY GENİŞLİK VE DERİNLİK KONTROLÜ",
      "Text": "Sealing \nİç ve Dış 3.00 mm düşük olmamalı, \nİç Derinlik Min 0.90 Max 1.85, \nDış Derinlik Min 0.50 Max 1.15 \nolmalı",
      "Active": true
    },
    {
      "Id": 13,
      "Title": "CROSS ARALIK ÖLÇÜMÜ",
      "Text": "Kısa cross orta bölgede mesafe 360±1 olmalı Uzun Crosslar arası mesafe Min.312 mm olmalı",
      "Active": true
    },
    {
      "Id": 14,
      "Title": "COOLING SIDE DELİK SAYILARI",
      "Text": "1-(120 Ejot)..............\n2-(12 Adet M8)..............\n3-(8 Adet Ø19.20)..............\n4-(18 Adet Burç)..............\n5-(8 Adet Yan etek)..............\n6-(16 Adet arka kısım)..............\n",
      "Active": true
    },
    {
      "Id": 25,
      "Title": "T CROS DELİK KAÇIKLIK KONTROLÜ",
      "Text": "Delik kenar arası mesafe 2.00mm düşük olmamalıdır.",
      "Active": true
    },
    {
      "Id": 39,
      "Title": "T PROFİL VE CROSS POKA YOKE VE DELİK VARLIK KONTROL",
      "Text": "T profilde 2 adet Ø 7.00 1 ve 2 crosslarda Ø7.00 poka yoke delikler doğru olmalı. Yan Ejot delikleri varlık kontrol mastarı ile kontrol edilecek.",
      "Active": true
    },
    {
      "Id": 31,
      "Title": "CROSS VE PROFİL M6 DİŞ KONTROL",
      "Text": "Profil kenar kısımlarında 9 adet M6 dişler olmalı. Tcros bölgesinde 9 adet M6 diş olmalı.",
      "Active": true
    },
    {
      "Id": 40,
      "Title": "MASKELEME",
      "Text": "T-Crossda 16 Adet Dış 4 Adet iç kısımda M6 dişler maskeleme yapılarak boya alması engellenmeli.",
      "Active": true
    },
    {
      "Id": 22,
      "Title": "Ön Döküm Delik Kontrol",
      "Text": "Döküm iç kısmında 3 adet Ø5.50 dış köşe kısmında 1 adet Ø10 delik, 4 adet M10 ve 3 adet M6 açılmış olmalı.",
      "Active": true
    },
    {
      "Id": 41,
      "Title": "KISA MODÜL KONTROL",
      "Text": "Kısa crosslar arasında 2 bölgede modül montabilite kontrolleri yapılacak.",
      "Active": true
    },
    {
      "Id": 7,
      "Title": "GENEL GÖRSEL KONTROL TEMİZLİK",
      "Text": "Parça yüzeylerinde derin çizik, ezik, vuruk deformasyon, vb. hatalar olmamalı. Detay parçalar tam olmalı.",
      "Active": true
    },
    {
      "Id": 5,
      "Title": "KONTROL TARİHİ/VARDİYA VE SAATİ",
      "Text": "Kontrol başlangıç Bitiş kaydedilecek",
      "Active": true
    },
    {
      "Id": 8,
      "Title": "KAYNAK KONTROLLERİ",
      "Text": "Burç ve crosslarda kaynak eksikliği, süresizlik, porozite, Max. 3.00 mm, eksen kaçıklığı olmamalı.",
      "Active": true
    },
    {
      "Id": 9,
      "Title": "Ø 44 BÖLGESİ KONTROL",
      "Text": "Conta kanalında kaçıklık 2.50 mm altı olmalı.\n\n8 adet M5 diş açılmış olmalı. Mastar ile M5 delik pozisyon kontrolü yapılmalı.",
      "Active": true
    },
    {
      "Id": 10,
      "Title": "BURÇ KONTROL (COOLING TARAFI)",
      "Text": "Burç Yükseklik 20±0.50\n\nİlk ve son burçlarda delik çap kontrolü (Ø16±0.20)",
      "Active": true
    },
    {
      "Id": 11,
      "Title": "77 ÖLÇÜSÜ",
      "Text": "76.50 ile 77.20 arası olmalıdır.",
      "Active": true
    },
    {
      "Id": 12,
      "Title": "122 ÖLÇÜSÜ",
      "Text": "121.50 ile 122.50 arası olmalıdır.",
      "Active": true
    },
    {
      "Id": 15,
      "Title": "EJOT DELİK DERİNLİK KONTROLÜ",
      "Text": "Ejot deliklerden örnekleme 12 adet 4.70 * 15.00 mm çap ve derinlik kontrolü yapılacak, köşelerden ve orta kısımlarından kontrol yapılmalı. Yan Ejot delikleri varlık kontrol mastarı ile kontrol edilecek.",
      "Active": true
    },
    {
      "Id": 17,
      "Title": "COOLİNG SİDE BİRLEŞTİRME",
      "Text": "Cooling side kapak oturmalı delikler karşılamalı",
      "Active": true
    },
    {
      "Id": 18,
      "Title": "M8 DERİNLİK KONTROLÜ",
      "Text": "12 adet M8 diş derinlikleri (15.00 mm) uygun olmalı, \nmontabilite civata yüzeyleri oturmalı.",
      "Active": true
    },
    {
      "Id": 19,
      "Title": "MASKELEME",
      "Text": "Crosslarda 12 Adet M8 dişler ve Tucker üzerinde maskeleme yapılacak boya alması engellenmeli.",
      "Active": true
    },
    {
      "Id": 20,
      "Title": "BURÇ KAÇIKLIK KONTROL",
      "Text": "Üst yüzeylerde 5.00 mm altında olmamalı.",
      "Active": true
    },
    {
      "Id": 21,
      "Title": "MASKELEME DÖKÜM BÖLGESİ",
      "Text": "     ",
      "Active": true
    },
    {
      "Id": 23,
      "Title": "GRUNDİNG BOLT KONTROLÜ (TUCKER)",
      "Text": "Grunding Bolt ve üzerinde somun takılı olmalı.",
      "Active": true
    },
    {
      "Id": 24,
      "Title": "PARÇA ÇEVİRME",
      "Text": "Parçayı Cover tarafına deforme etmeden çevrilmeli.",
      "Active": true
    },
    {
      "Id": 26,
      "Title": "BURÇ KAÇIKLIK KONTROL (COOLİNG TARAFI)",
      "Text": "Ön döküm burç 2.00 mm \nL profil burç 1.00 mm \naltında olmamalı",
      "Active": true
    },
    {
      "Id": 28,
      "Title": "CROSS DELİK SAYILARI KONTROLÜ",
      "Text": "1-(101 Adet Ejot)\n2-(10xØ5.50/2xØ7/8xM6)(T cross)\n3-(25 Adet 1. Sıra)\n4-(30 Adet 2. Sıra)\n5-(25 Adet 3. Sıra)\n6-(31 Adet 4. Sıra)\n7-(17 Adet 2\u002B8\u002B5\u002B2 kısa)",
      "Active": true
    },
    {
      "Id": 29,
      "Title": "UZUN CROSS MODÜL KONTROL",
      "Text": "Uzun Crosslar arasında 8 bölgede modül montabilite kontrolleri yapılacak.",
      "Active": true
    },
    {
      "Id": 30,
      "Title": "GENEL GÖRSEL KONTROL TEMİZLİK",
      "Text": "Parça yüzeylerinde derin çizik, ezik, vuruk deformasyon, vb. hatalar olmamalı. Detay parçalar tam olmalı.",
      "Active": true
    },
    {
      "Id": 32,
      "Title": "EJOT DELİK DERİNLİK KONTROLÜ",
      "Text": "Ejot deliklerden örnekleme 12 adet 4.70 * 13.00 mm çap ve derinlik kontrolü yapılacak, köşelerden ve orta kısımlardan kontrol yapılmalı.",
      "Active": true
    },
    {
      "Id": 33,
      "Title": "COVER BİRLEŞTİRME",
      "Text": "Cover Montaj Hazırlık, \n\nCover Montaj, \n\nCover kapak oturmalı ejot delikler \nkarşılamalı. Eksik delik olmamalı.",
      "Active": true
    },
    {
      "Id": 34,
      "Title": "COVER EVLENDİRME DEMONTE",
      "Text": "Cover montaj aparatları sökülecek ve üzerinden alınacak ve sehpasına konulacak.",
      "Active": true
    },
    {
      "Id": 35,
      "Title": "HAVA İLE TEMİZLEME",
      "Text": "Cross kanalları içleri ve yüzeyler hava ile çapaktan arındırılmalı.",
      "Active": true
    },
    {
      "Id": 36,
      "Title": "MASKELEME",
      "Text": "Crosslarda 8 Adet M8 dişler maskeleme yapılarak boya alması engellenmeli.",
      "Active": true
    },
    {
      "Id": 37,
      "Title": "Ø 5.50 DERİNLİK KONTROL",
      "Text": "Her crostan örnekleme 2 adet 16.50 derinlik kontrolü yapılacak.",
      "Active": true
    },
    {
      "Id": 38,
      "Title": "RAPOR KAPATMA KASALAMA",
